{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"outDir": "./dist",
		"declaration": true,
		"declarationMap": false,
		"emitDeclarationOnly": false,
		"noEmit": false,
		"module": "ESNext",
		"target": "ES2019"
	},
	"include": [
		"src/**/*"
	],
	"exclude": [
		"demo",
		"node_modules",
		"**/*.test.*"
	]
}